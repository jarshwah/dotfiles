# PATH

# Brew first
PATH="/usr/local/bin:/usr/local/sbin:${PATH}"
# Use GNU tools rather than Apple ones
PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
# NPM
PATH="/usr/local/share/npm/bin:${PATH}"
# Local bin first!
PATH="$HOME/bin:$PATH"
# Link oracle client libraries
PATH="$PATH:/usr/local/oracleclient/instantclient_12_1"
export PATH
